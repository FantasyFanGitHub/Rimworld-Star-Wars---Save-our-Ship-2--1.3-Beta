<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>

      <li Class="OuterRim.PatchOperation_SettingActive">
        <settings>
          <li>era_cloneWars</li>
        </settings>
        <active Class="PatchOperationSequence">
          <success>Always</success>
          <operations>
            <!--<li Class="OuterRim.PatchOperation_OnDemand">
              <source>1.3/OnDemand/Era</source>
              <folders>
                <li>Era_CloneWars</li>
              </folders>
            </li>-->

            <li Class="PatchOperationAdd">
              <xpath>/Defs/FactionDef[defName="O21_OR_CloneBattalionPhaseTwo"]/pawnGroupMakers</xpath>
              <value>
                <li>
                  <kindDef>Combat</kindDef>
                  <options>
                    <CEC_JudicialForces>80</CEC_JudicialForces>
                  </options>
                </li>
              </value>
            </li>
          </operations>
        </active>
      </li>

