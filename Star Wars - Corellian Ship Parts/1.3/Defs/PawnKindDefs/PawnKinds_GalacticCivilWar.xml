<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <!--TIE Pilot-->
  <PawnKindDef ParentName="O21_OR_EMPStormBase">
		<defName>CEC_ImpTIEPilot</defName>
		<label>TIE pilot</label>
		<labelPlural>TIE pilots</labelPlural>
		<isFighter>true</isFighter>
		<weaponTags Inherit="false">
      <li>ImperialLight</li>
			<li>ImperialStandard</li>
		</weaponTags>
		<apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
		<apparelTags Inherit="False">
			<li>ORBodysuit_Warm</li>
			<li>ImperialTIE</li>
			<li>ORThermalDetBelt</li>
		</apparelTags>
		<apparelRequired>
			<li>CEC_Bodysuit_Warm</li>
			<li>CEC_TIEPilotSuit</li>
			<li>CEC_TIEPilotHelmet</li>
		</apparelRequired>
		<combatPower>80</combatPower>
		<modExtensions>
			<li Class="O21Toolbox.PawnKindExt.DefModExt_ExtendedPawnKind">
				<additionalHediffs>
					<li>
						<hediff>O21_OR_StormtrooperTraining</hediff>
						<severityRange>0.2~0.68</severityRange>
					</li>
				</additionalHediffs>
				<randomAdditionalHediff>false</randomAdditionalHediff>
				<clearChronicIllness>True</clearChronicIllness>
				<clearAddictions>True</clearAddictions>
			</li>
		</modExtensions>
	</PawnKindDef>
  
