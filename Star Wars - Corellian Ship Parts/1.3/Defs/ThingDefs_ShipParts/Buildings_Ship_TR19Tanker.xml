  <ThingDef ParentName="CEC_RebTankerTankBase" MayRequire="BK.PipedMilk">
    <defName>Ship_TR19MediumTanker_Tank_Milk</defName>
    <label>TR-19 medium tanker tank (milk)</label>
    <description>Stores and cools milk for later use (BK's Piped Milk). Requires power.</description>
    <graphicData>
      <color>(255,255,255)</color><!--Milk-->
    </graphicData>
    <researchPrerequisites>
      <li>Electricity</li>
    </researchPrerequisites>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceStorage">
        <pipeNet>BK_MilkNet</pipeNet>
        <storageCapacity>8000</storageCapacity>
        <barSize>(1.3, 0.25)</barSize>
        <margin>0.1</margin>
		    <contentRequirePower>true</contentRequirePower>
      </li>
	    <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>150</basePowerConsumption>
      </li>
    </comps>
  </ThingDef>
  
