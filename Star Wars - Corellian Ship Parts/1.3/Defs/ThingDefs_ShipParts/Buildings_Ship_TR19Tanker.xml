  <ThingDef ParentName="CEC_RebTankerTankBase">
    <defName>Ship_TR19MediumTanker_Tank_Tibannah</defName>
    <label>TR-19 medium tanker tank (tibannah)</label>
    <description>Stores tibanna for later use. Will explode if damaged.</description>
    <graphicData>
      <color>(174,66,169)</color><!--Tibannah-->
    </graphicData>
    <researchPrerequisites>
      <li>O21_OR_BasicHypertech</li>
    </researchPrerequisites>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceStorage">
        <pipeNet>O21_OR_TibannaNetwork</pipeNet>
          <storageCapacity>8000</storageCapacity>
          <barSize>(1.3, 0.25)</barSize>
          <margin>0.1</margin>
          <centerOffset>(0.5, 0, 0)</centerOffset>
        </li>
        <li Class="PipeSystem.CompProperties_ExplosiveContent">
          <explosiveMaxRadius>30</explosiveMaxRadius><!-- Vanilla options below -->
          <explosiveDamageType>Flame</explosiveDamageType>
          <destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
          <startWickHitPointsPercent>0.05</startWickHitPointsPercent>
          <preExplosionSpawnChance>1</preExplosionSpawnChance>
          <wickTicks>70~150</wickTicks>
        </li>
      </comps>
  </ThingDef>
  
