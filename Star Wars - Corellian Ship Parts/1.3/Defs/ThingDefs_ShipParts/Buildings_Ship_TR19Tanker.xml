  <ThingDef ParentName="CEC_RebTankerTankBase" MayRequire="Dubwise.DubsBadHygiene" DBHLite="true">
    <defName>Ship_TR19MediumTanker_Tank_DubsWater</defName>
    <label>TR-19 medium tanker tank (water)</label>
    <description>Stores water for later use (Dubs Bad Hygiene). If the contained water becomes contaminated, the tank must be drained.</description>
    <graphicData>
      <color>(35,147,191)</color><!--Dubs Water-->
    </graphicData>
    <researchPrerequisites>
      <li MayRequire="dbhlitemode">LargeWaterPumps</li>
    </researchPrerequisites>
    <comps>
      <li Class="DubsBadHygiene.CompProperties_WaterStorage">
				<WaterStorageCap>80000</WaterStorageCap>
			</li>
      <li Class="DubsBadHygiene.CompProperties_Pipe">
				<mode>Sewage</mode>
			</li>
    </comps>
  </ThingDef>
          
