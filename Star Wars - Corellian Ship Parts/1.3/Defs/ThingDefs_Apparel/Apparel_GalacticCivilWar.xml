  <ThingDef ParentName="OR_LightArmorFabricatedBase">
    <defName>CEC_RebelPilotSuit</defName>
    <label>rebel pilot suit</label>
    <description>An lightly armoured pilot suit used by the Rebel Alliance in the Starfighter Corps.</description>
    <graphicData>
      <texPath>Apparel/RebelPilot/Apparel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(255, 255, 255)</color>
    </graphicData>
    <recipeMaker>
      <researchPrerequisite>O21_OR_RebelArmor</researchPrerequisite>
    </recipeMaker>
    <costList Inherit="False">
      <O21_OR_Durasteel>30</O21_OR_Durasteel>
      <O21_OR_Durafiber>25</O21_OR_Durafiber>
      <O21_OR_ComponentHypertech>2</O21_OR_ComponentHypertech>
    </costList>
    <apparel>
      <tags Inherit="False">
        <li>RebelPilot</li>
      </tags>
      <wornGraphicPath>Apparel/RebelPilot/Apparel</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options" Inherit="False">
      <options>
        <li>
          <weight>20</weight>
          <only>(242, 124, 28)</only>
        </li>
        <li>
          <weight>3</weight>
          <only>(238, 55, 55)</only>
        </li>
        <li>
          <weight>8</weight>
          <only>(17, 164, 22)</only>
        </li>
        <li>
          <weight>5</weight>
          <only>(56, 56, 200)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>

  <!--TIE Pilot-->
  <ThingDef ParentName="OR_MediumHelmetFabricatedBase">
    <defName>CEC_TIEPilotHelmet</defName>
    <label>TIE pilot helmet</label>
    <description>An airtight, armoured helmet used by the Galactic Empire in the Starfighter Corps.</description>
    <graphicData>
      <texPath>Apparel/TIEPilot/Helmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <color>(255, 255, 255)</color>
    </graphicData>
    <recipeMaker>
      <researchPrerequisite>O21_OR_ImperialArmor</researchPrerequisite>
    </recipeMaker>
    <equippedStatOffsets>
      <ShootingAccuracyPawn>-2</ShootingAccuracyPawn>
    </equippedStatOffsets>
    <statBases>
      <Insulation_Cold>50</Insulation_Cold>
    </statBases>
    <apparel>
      <tags Inherit="False">
        <li>ImperialApparel</li>
        <li>ImperialTIE</li>
        <li>EVA</li>
      </tags>
      <wornGraphicPath>Apparel/TIEPilot/Helmet</wornGraphicPath>
      <useWornGraphicMask>false</useWornGraphicMask>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options" Inherit="False">
      <options>
        <li>
          <weight>10</weight>
          <only>(255, 255, 255)</only>
        </li>
      </options>
