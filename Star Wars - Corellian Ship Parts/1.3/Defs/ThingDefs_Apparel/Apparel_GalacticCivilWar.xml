<Defs>
  
  <!--Rebel Captain-->
  <ThingDef ParentName="OR_UniformBodyFabricatedBase">
    <defName>CEC_RebelCaptainUniform</defName>
    <label>rebel captain uniform</label>
    <description>A durable uniform, most famously used by the Rebel Alliance.</description>
    <graphicData>
      <texPath>Apparel/RebelOfficer/Apparel_Male_south</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(255, 255, 255)</color>
    </graphicData>
    <recipeMaker>
      <researchPrerequisite>O21_OR_RebelArmor</researchPrerequisite>
    </recipeMaker>
    <apparel>
      <tags Inherit="False">
        <li>RebelCaptain</li>
      </tags>
      <wornGraphicPath>Apparel/RebelOfficer/Apparel</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(255, 255, 255)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  
